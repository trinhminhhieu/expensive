  section.header-box
    .row
      .backgroundbox
        // nen hop tim kiem chung
        .backgroundbox__boxmenu
          // menu-form begin
          .menuform
            .menuform__box
              button.menuform__button(type='button', href='#flkight')
                span.menuform__label &#x2708; Flights
            .menuform__box
              button.menuform__button(type='button', href='#flight')
                span.menuform__label &#x1F6F3; Cruises
            .menuform__box
              button.menuform__button(type='button')
                span.menuform__label &#x1F3D9; Hotels
          // menu-form end
          // form search begin
          form.form(action='#')
            #flight.form__search
              .form__input
                label.form__labeltitle Fly from: 
                input.form__inputtext(type='text', placeholder='Place origin')
                span.form__icon.ti-location-pin
              .form__icon-box
                span.form__icon-center.ti-arrow-right
              .form__input
                label.form__labeltitle Fly to: 
                input.form__inputtext(type='text', placeholder='Place to')
                span.form__icon.ti-location-pin
            // ////line2///
            .form__search-2
              .form__input-2
                label.form__labeltitle Date Departing: 
                input.form__inputtext-2(type='date', name='bday', placeholder='Place origin')
                span.form__icon.ti-calendar
              .form__input-2
                label.form__labeltitle Date Returming: 
                input.form__inputtext-2(type='date', name='bday', placeholder='Place origin')
                span.form__icon.ti-calendar
              .form__input-2
                label.form__labeltitle Airlines: 
                input.form__inputtext-2(type='text', placeholder='AirAsia')
                span.form__icon.dropdown.ti-angle-down
            .menuform-2
              .menuform-2__box-2
                button.menuform-2__button-2(type='button')
                  span.menuform-2__label-2 Search
