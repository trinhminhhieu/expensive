extends base

block content

  // begin menuphones
  .navigator
    input#toggle.navigator__click(type='checkbox')
    label.navigator__button(for='toggle')
      span.navigator__menuicon.ti-menu
    .navigator__background
    nav.navigator__navul
      ul.navigator__listitem
        li.navigator__item
          a.navigator__link(href='#') Travel sites
        li.navigator__item
          a.navigator__link(href='#') Flights
        li.navigator__item
          a.navigator__link(href='#') Best place
        li.navigator__item
          a.navigator__link(href='#') Cruises
        li.navigator__item
          p.dropbtn(onclick='myFunction()') Account
        #myDropdown.dropdown-content
          a.login-trigger.datapopup(onclick='dataPopup()', href='#') Login
          a.login-trigger.datapopup(onclick='dataregisterPopup()', href='#') Rgister
  // end menuphone
  // Trigger
  #overlayregister.overlay(style='display:none;')
    #loginpopup.overlay__modalzo.fadex(role='dialog')
      .modal-dialog
        .modal-mycontent
          .modal-body
            span#closewindow.closeo(data-dismiss='modalzo', onclick='dataCloser()') &times;
            main
              .register-form
                h2.heading-secondary-register Onemytrip create account
                p#messnotice.message
                form.form
                  //
                    <div class="form__group"><label class="form__label" for="email">Name *</label><input
                    class="form__inputo" id="name" type="text" placeholder="youname" required="required" /></div>
                  .form__group
                    label.form__label(for='email') Email address *
                    input#email.form__inputo(type='email', placeholder='you@example.com', required='required')
                  //
                    <div class="form__group ma-bt-md"><label class="form__label" for="password">Password *</label><input
                    class="form__inputo" id="password" type="current-password" placeholder="••••••••"
                    required="required" minlength="8" /></div>
                  .form__group
                    .policy
                      .checkagree
                        label.fontinput-checkbox(for='#')
                          input.agreecheck(type='checkbox')
                          span.checkmark
                      .textpolicy
                        p.textsummarypolicy
                          | Join 
                          a(href='#') OneMyTrip
                          | . opens in a new window By
                          |                                       joining I accept all 
                          a(href='/policy') terms and conditions.
                  .form__group
                    p.textaldeary Already have an account?
                    a.textforgotpass(href='#') Login
                  .form__group
                    .ortext
                      .textdeconrate--text
                        p.or OR Register with
                      hr
                  .form__group
                    .btn-social
                      button.btn-label-1
                        span
                          img.labelimg(src='https://a.travel-assets.com/gss-loader/user/gss-content/images/google-logo.svg', alt='google-logo')
                          fonts.textlabelz Register with Google
                      button.btn-label-2
                        span
                          img.labelimg(src='https://a.travel-assets.com/dms-svg/social/logos-facebook-invert.svg', alt='google-logo')
                          fonts.textlabelz-2 Register with Facebook
                  .form__group
                    button.btno.btn--green Register
  #overlay.overlay(style='display:none;')
    #loginpopup.overlay__modalzo.fadex(role='dialog')
      .modal-dialog
        .modal-mycontent
          .modal-body
            span#closewindow.closeo(data-dismiss='modalzo', onclick='dataClose()') &times;
            main
